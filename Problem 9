#include <iostream>
#include <iomanip>
#include <conio.h>

using namespace std;

int main()
{
	int rows, columns, r, c;

	cout << "Enter number of Rows: ";
	cin >> rows;

	cout << "Enter number of Columns: ";
	cin >> columns;

		r = 1;
		while ( r <= rows)
		{
			r++;
				for (c = 1; c <= columns; c++)
				{
					cout << "*";
				}

			cout << "\n";
		}

	_getch();
	return 0;
}



